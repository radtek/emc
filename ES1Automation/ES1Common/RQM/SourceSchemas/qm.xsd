									<xs:annotation>
										<xs:documentation xml:lang="en">[READ-ONLY] The URL of the test plan.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute ref="vega:id">
									<xs:annotation>
										<xs:documentation xml:lang="en">[READ-ONLY] UUID of the test plan.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
