										<xs:documentation xml:lang="en">[DEPRECATED] [READ-ONLY]</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>	
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
