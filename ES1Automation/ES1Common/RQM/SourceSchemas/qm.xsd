							</xs:annotation>
					  	</xs:element>
						
					  	<xs:choice>
					  	<xs:element ref="buildschedule">
					  		<xs:annotation>
								<xs:documentation xml:lang="en">[READ-ONLY] Schedule to be triggerd on build completion</xs:documentation>
							</xs:annotation>
						</xs:element>
					  	<xs:element ref="onetimeschedule">
