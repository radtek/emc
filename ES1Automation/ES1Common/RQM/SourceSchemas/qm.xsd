				</xs:annotation>
			</xs:element>
			<xs:element ref="expiryDate">
				<xs:annotation>
					<xs:documentation xml:lang="en">[READ-ONLY] Time when job shcedule should be triggered</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
   <xs:complexType name="timeschedule">
   		<xs:complexContent>
