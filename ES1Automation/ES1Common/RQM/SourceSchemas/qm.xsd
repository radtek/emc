								<xs:documentation xml:lang="en">[READ-ONLY] The contributor that owns the remote script.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="1" minOccurs="0" name="locked" type="xs:boolean">
							<xs:annotation>
							  <xs:documentation xml:lang="en">[READ-ONLY] Boolean indicating the remote script is locked.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="1" minOccurs="0" ref="approvals">
