						<xs:any maxOccurs="unbounded" minOccurs="0" processContents="skip">
							<xs:annotation>
								<xs:documentation xml:lang="en">This is a placeholder for any user defined sections that have been added to the test case.</xs:documentation>
							</xs:annotation>
						</xs:any>
						
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="testscript">
