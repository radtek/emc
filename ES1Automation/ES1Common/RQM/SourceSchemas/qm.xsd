									<xs:annotation>
										<xs:documentation xml:lang="en">[READ-ONLY] A marker as to whether this is the current suite log.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="testsuitelog">
							<xs:annotation>
								<xs:documentation xml:lang="en">A reference to the results of an execution run.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="href" type="xs:anyURI" use="required">
									<xs:annotation>
										<xs:documentation xml:lang="en">[READ-ONLY] The URL of the suite log.</xs:documentation>
									</xs:annotation>
