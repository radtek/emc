							<xs:annotation>
								<xs:documentation xml:lang="en">Actual result state of Execution Result e.g. com.ibm.rqm.execution.common.state.passed</xs:documentation>								
							</xs:annotation>
						</xs:element>
						<xs:element ref="dc:creator">
